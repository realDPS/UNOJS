{
  "name": "uno.js",
  "version": "0.0.0",
  "scripts": {
    "start": "node ./server/index.js",
    "watch": "tsc -w --esModuleInterop ./server/index.ts ./type.d.ts",
    "server": "nodemon ./server/index.js",
    "dev": "vite",
    "build": "vite build && tsc --esModuleInterop --removeComments ./server/index.ts ./type.d.ts",
    "serve": "vite preview"
  },
  "engines": {
    "node": "16.0.0"
  },
  "devDependencies": {
    "@rollup/plugin-strip": "^2.0.1",
    "@sveltejs/vite-plugin-svelte": "^1.0.0-next.10",
    "@types/express": "^4.17.11",
    "svelte": "^3.38.2",
    "svelte-preprocess": "^4.7.3",
    "typescript": "^4.2.4",
    "vite": "^2.3.3",
    "vite-tsconfig-paths": "^3.3.11"
  },
  "dependencies": {
    "cors": "^2.8.5",
    "express": "^4.17.1",
    "socket.io": "^4.1.2",
    "socket.io-client": "^4.1.2"
  }
}